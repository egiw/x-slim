{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router"],
    "homepage": "http://github.com/codeguy/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "openwall/phpass",
                "version": "0.3",
                "dist": {
                    "url": "http://www.openwall.com/phpass/phpass-0.3.tar.gz",
                    "type": "tar"
                },
                "autoload": {
                    "classmap": ["PasswordHash.php"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": "1.*",
        "slim/slim": "2.*",
        "slim/views": "0.*",
        "twig/twig": "1.*",
        "doctrine/orm": "2.4.*",
        "beberlei/DoctrineExtensions": "dev-master",
        "dompdf/dompdf": "dev-master",
        "respect/validation": "dev-master",
        "jpgraph/jpgraph": "dev-master",
        "swiftmailer/swiftmailer": "5.*",
        "openwall/phpass": "0.3",
        "meenie/javascript-packer": "dev-master",
        "umpirsky/twig-gettext-extractor": "1.1.*",
        "gedmo/doctrine-extensions": "v2.3.9"
    },
    "autoload": {
        "classmap": ["vendor/jpgraph/jpgraph/lib/JpGraph"],
        "psr-0": {
            "": ["src/", "lib/"]
        }
    }
}
